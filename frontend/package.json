{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test --workers=1",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed --workers=1",
    "test:e2e:report": "PW_HTML_REPORT=1 playwright test --workers=1 && playwright show-report --host=127.0.0.1",
    "test:e2e:ci": "CI=1 playwright test --workers=1",
    "test:e2e:automated": "playwright test --reporter=list,junit --workers=1",
    "test:accessibility": "playwright test --grep=@accessibility --workers=1",
    "test:all": "npm run test && npm run test:e2e:automated",
    "test:all:with-report": "npm run test && npm run test:e2e:report",
    "playwright:install": "playwright install",
    "test:cleanup": "pkill -f \"vite.*preview\" || taskkill /F /IM node.exe || true",
    "test:integration": "jest --testPathPattern=integration --testTimeout=30000",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:all-working": "npm run test:unit && npm run test:integration"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.79.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.15.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "vis-network": "^9.1.10"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.9.0",
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.48.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.27",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.8.1",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
